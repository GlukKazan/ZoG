(define prepare-promote
  (set-flag r? (piece? PawnRook))
  (set-flag k? (piece? PawnKnight))
  (set-flag b? (piece? PawnBishop))
  (set-flag q? (piece? PawnQueen))
  (set-flag h? (piece? PawnHelgi))
)

(define promote
  (if (flag? $1)
      (change-type $2)
  )
)

(define promote-all
  (if (in-zone? promotion-zone)
      (promote r? Rook)
      (promote k? Knight)
      (promote b? Bishop)
      (promote q? Queen)
      (promote h? Helgi)
   else
      (set-attribute not-moved? false) 
  )
)

(define prepare-unprom
  (set-flag epr? (and (or (piece? PawnRook)   (piece? RookP))   enemy?))
  (set-flag epk? (and (or (piece? PawnKnight) (piece? KnightP)) enemy?))
  (set-flag epb? (and (or (piece? PawnBishop) (piece? BishopP)) enemy?))
  (set-flag epq? (and (or (piece? PawnQueen)  (piece? QueenP))  enemy?))
  (set-flag eph? (and (or (piece? PawnHelgi)  (piece? Helgi))   enemy?))
  (set-flag pr?  (or (and (piece? PawnRookE)   enemy?) (and (piece? RookP)   friend?)))
  (set-flag pk?  (or (and (piece? PawnKnightE) enemy?) (and (piece? KnightP) friend?)))
  (set-flag pb?  (or (and (piece? PawnBishopE) enemy?) (and (piece? BishopP) friend?)))
  (set-flag pq?  (or (and (piece? PawnQueenE)  enemy?) (and (piece? QueenP)  friend?)))
  (set-flag ph?  (or (and (piece? PawnHelgiE)  enemy?) (and (piece? Helgi)   friend?)))
  (set-flag er?  (and (piece? Rook)    enemy?))
  (set-flag ek?  (and (piece? Knight)  enemy?))
  (set-flag eb?  (and (piece? Bishop)  enemy?))
  (set-flag eq?  (and (piece? Queen)   enemy?))
  (set-flag eh?  (and (piece? King)    enemy?))
  (set-flag fer? (and (piece? RookE)   enemy?))
  (set-flag fek? (and (piece? KnightE) enemy?))
  (set-flag feb? (and (piece? BishopE) enemy?))
  (set-flag feq? (and (piece? QueenE)  enemy?))
)

(define unprom
  (if (flag? $1)
      (change-type $2)
      (set-attribute not-moved? false) 
      change-owner
  )
)

(define unprom-all
  (unprom epr? PawnRookE)   (unprom pr?  PawnRook)
  (unprom epk? PawnKnightE) (unprom pk?  PawnKnight)
  (unprom epb? PawnBishopE) (unprom pb?  PawnBishop)
  (unprom epq? PawnQueenE)  (unprom pq?  PawnQueen)
  (unprom eph? PawnHelgiE)  (unprom ph?  PawnHelgi)
  (unprom er?  RookE)       (unprom fer? Rook)
  (unprom ek?  KnightE)     (unprom fek? Knight)
  (unprom eb?  BishopE)     (unprom feb? Bishop)
  (unprom eq?  QueenE)      (unprom feq? Queen)
  (unprom eh?  KingE)
)

(define check-split
  (if (on-board? down)
      (verify (and
          (not-piece? PawnRookE   down)
          (not-piece? PawnKnightE down)
          (not-piece? PawnBishopE down)
          (not-piece? PawnQueenE  down)
          (not-piece? PawnHelgiE  down)
          (not-piece? RookE       down)
          (not-piece? KnightE     down)
          (not-piece? BishopE     down)
          (not-piece? QueenE      down)
      ))
  )
)

(define check-pawn
  (verify (in-zone? $1))
  (verify (or (piece? PawnRook) (piece? PawnKnight) (piece? PawnBishop) (piece? PawnQueen) (piece? PawnHelgi)))
)

(define check-king
  (verify (in-zone? $1))
  (verify (piece? King))
)

(define check-knight
  (verify (in-zone? $1))
  (verify (or (piece? Knight) (piece? Helgi)))
)

(define check-slide
  (verify (in-zone? $1))
  (verify (or (piece? $2) (piece? Queen) (piece? Helgi)))
)

(define cover
  (verify (or (not-piece? King) enemy?))
  (promote-all) to
  (while (and (on-board? down) not-empty?)
      (prepare-unprom) cascade down
      (unprom-all) to
  )
)

(define lift
  (if (and (on-board? down) (not-empty? down))
      cascade down (prepare-promote) from
      up (promote-all) to down
  )
  (while (and (on-board? down) (not-empty? down))
      cascade down from
      up to down
  )
)

(define pawn-jump (
  (check-pawn top-zone)
  (verify not-moved?) (verify (empty? down))
  n (verify empty?)
  n (verify not-enemy?)
  (cover) add
))

(define pawn-1-shift (
  (check-pawn top-zone)
  (check-split) (prepare-promote)
  mark n (verify not-enemy?)
  (cover) back (lift) add
))

(define pawn-1-step (
  (check-pawn top-zone)
  (check-split) (prepare-promote)
  mark $1 (verify enemy?)
  (cover) back (lift) add
))

(define knight-1-step (
  (check-knight top-zone)
  (check-split) mark $1 $2 
  (cover) back (lift) add
))

(define common-1-slide-1 (
  (check-slide top-zone $2)
  (check-split) mark $1 
  (cover) back (lift) add
))

(define common-1-slide-2 (
