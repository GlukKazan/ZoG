(define king-step (
  (check-king top-zone)
  (check-split)
  mark $1 (cover) back (lift) add
))

(define O-O ( 
  (verify not-moved?)
  e (verify empty?)
  e (verify empty?)
  cascade
  e (verify (and friend? (piece? Rook) not-moved?))
  from back (verify not-attacked?)
  e (verify not-attacked?)
  to (set-attribute not-moved? false)
  e (set-attribute not-moved? false)
  add
))

(define O-O-O ( 
  (verify not-moved?)
  w (verify empty?)
  w (verify empty?)
  cascade w (verify empty?)
  w (verify (and friend? (piece? Rook) not-moved?))
  from back  (verify not-attacked?)
  w (verify not-attacked?)
  to (set-attribute not-moved? false)
  w (set-attribute not-moved? false)
  add
))

