(define not-ka
  mark
  (while (on-board? $1)
     $1
     (verify (not-piece? Ka))
  )
  back
)

(define ka-blue (
  (set-position-flag from-pos true)
  (verify (in-zone? blue-zone))
  $1 (verify empty?)
  (common-slide)
  (create Blue Ka)
  add
))

(define ka-red (
  (set-position-flag from-pos true)
  (verify (not-in-zone? blue-zone))
  $1 (verify empty?)
  (not-ka n) (not-ka s)
  (not-ka w) (not-ka e)
  (common-slide)
  (create Red Ka)
  add
))

(define check-dir
  mark
  (set-flag not-done true)
  (while (and (on-board? $1) (flag? not-done))
      $1
      (if (and not-empty? (not-position-flag? from-pos))
          (if (piece? $2)
              (if (flag? is-odd)
                  (set-flag is-odd false)
               else
                  (set-flag is-odd true)
              )
              (add-points)
              capture
           else
              (set-flag not-done false)
          )
      )
  )
  back
)

(define common-slide
  (set-flag is-odd false)
  (check-dir $2 $4)
  (check-dir $3 $4)
  (verify (flag? is-odd))
  (set-points)
)

