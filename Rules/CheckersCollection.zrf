(define clear-all
   mark  
   a0 
   (while (on-board? next) 
      next
      (if (piece? Checker)
          (set-attribute captured? false)
      )
   )
   back  
)

(define north-jump (
  $1
  (verify enemy?)
  (if (piece? King)
      (change-type Checker)
      (set-attribute captured? true)
   else
      capture
  )
  $1
  (verify empty?)
  (clear-all)
  (if (in-zone? promotion)
      (add-partial King jumptype)
   else
      (set-attribute captured? false)
      (add-partial continuetype)
  )
))

(define north-jump-continue (
  $1
  (verify enemy?)
  (if (piece? King)
      (change-type Checker)
      (set-attribute captured? true)
   else
      (verify (not captured?))
      capture
  )
  $1
  (verify empty?)
  (if (in-zone? promotion)
      (add-partial King jumptype)
   else
      (add-partial continuetype)
  )
))

(define north-king-jump (
  (while (empty? $1)
      $1
  )
  (verify (enemy? $1))
  $1
  (verify (empty? $1))
  $1
  (while empty?
      mark
      (while empty? 
        (opposite $1)
      ) 
      (if (piece? King)
          (change-type Checker)
          (set-attribute captured? true)
       else
          capture
      )
      back
      (clear-all)
      (add-partial continuetype)
      $1
  )
))

(define north-king-continue (
  (while (empty? $1)
      $1
      (verify not-last-from?)
  )
  (verify (enemy? $1))
  $1
  (verify (empty? $1))
  $1
  (while empty?
      mark
      (while empty? 
        (opposite $1)
      ) 
      (if (piece? King)
          (change-type Checker)
          (set-attribute captured? true)
       else
          (verify (not captured?))
          capture
      )
      back
      (add-partial continuetype)
      $1
  )
))

(define north-shift (
  (clear-all)
  $1
  (verify empty?)
  (if (in-zone? promotion)
      (add King)
   else
      (set-attribute captured? false)
      add
  )
))

(define north-king-shift (
  (clear-all)
  (while (empty? $1)
      $1
      add
  )
))

